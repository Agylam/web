    },
    "homepage": "https://github.com/Agylam/web#readme",
    "dependencies": {
        "bcrypt-ts": "^3.0.1",
        "dotenv": "^16.1.4",
        "fastify": "^4.17.0",
        "jsonwebtoken": "^9.0.0",
        "pg": "^8.11.0",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.32.0",
        "ts-md5": "^1.3.1",
        "ts-watch": "^1.0.8"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^9.0.2",
        "@types/node": "^20.2.5",
